<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
<display-name>CakeShopMangement</display-name>
<welcome-file-list>
<welcome-file>LoginPage.jsp</welcome-file>
</welcome-file-list>
<servlet>
	<servlet-name>myServlet</servlet-name>
	<servlet-class>Package1.CakeShopManagement</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>myServlet</servlet-name>
	<url-pattern>/login</url-pattern>
	<url-pattern>/new</url-pattern>
	<url-pattern>/saveUserDetails</url-pattern>
	<url-pattern>/availableCakes</url-pattern>
	<url-pattern>/MyOrders</url-pattern>
	<url-pattern>/displayOrderedCakes</url-pattern>
	<url-pattern>/makePayment</url-pattern>
	<url-pattern>/editListOfCakes</url-pattern>
</servlet-mapping>
</web-app>